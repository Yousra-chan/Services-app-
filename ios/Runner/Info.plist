<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Akhdem Li</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>akhdem_li</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	
	<!-- ===== MATCHING ANDROID MANIFEST FUNCTIONALITY ===== -->
	
	<!-- URL Schemes for external app integration -->
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<!-- Phone calls -->
		<string>tel</string>
		<string>telprompt</string>
		
		<!-- WhatsApp -->
		<string>whatsapp</string>
		<string>whatsapp-api</string>
		
		<!-- Web URLs -->
		<string>https</string>
		<string>http</string>
		
		<!-- SMS -->
		<string>sms</string>
		<string>smsto</string>
		
		<!-- Google Maps -->
		<string>comgooglemaps</string>
		<string>comgooglemaps-x-callback</string>
		<string>maps</string>
	</array>
	
	<!-- Location Services (matches ACCESS_FINE_LOCATION/ACCESS_COARSE_LOCATION) -->
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>This app needs access to your location to show nearby service providers and help you find services in your area.</string>
	
	<key>NSLocationAlwaysUsageDescription</key>
	<string>This app needs access to your location to provide you with relevant service providers based on your current location.</string>
	
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>This app needs access to your location to provide you with relevant service providers based on your current location.</string>
	
	<!-- Notifications (matches POST_NOTIFICATIONS) -->
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
	</array>
	
	<!-- Camera and Photos (for future features) -->
	<key>NSCameraUsageDescription</key>
	<string>This app needs camera access to take photos for your profile or service listings.</string>
	
	<key>NSPhotoLibraryUsageDescription</key>
	<string>This app needs photo library access to upload images for your profile or service listings.</string>
	
	<!-- Microphone (for future voice features) -->
	<key>NSMicrophoneUsageDescription</key>
	<string>This app needs microphone access for voice messages or calls within the app.</string>
	
	<!-- Background processing (matches WAKE_LOCK equivalent) -->
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
		<string>processing</string>
	</array>
</dict>
</plist>